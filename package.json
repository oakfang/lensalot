{
  "name": "lensalot",
  "version": "1.0.0",
  "description": "Lens approach to global react state via Hooks",
  "author": "oakfang",
  "license": "MIT",
  "repository": "oakfang/lensalot",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
